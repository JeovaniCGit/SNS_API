        "required": [
          "medicoDTO",
          "medicoUtilizadorId"
        ],
        "type": "object",
        "properties": {
          "medicoUtilizadorId": {
            "type": "integer",
            "format": "int32"
          },
          "medicoDTO": {
            "$ref": "#/components/schemas/AddAndGetMedicoDataDTO"
          }
        },
        "additionalProperties": false
      },
      "Especialidade": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "descri": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "HistoricoLaboral": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "instituiçãoid": {
            "type": "integer",
            "format": "int32"
          },
          "medicoid": {
            "type": "integer",
            "format": "int32"
          },
          "dataInicio": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "dataFim": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "instituição": {

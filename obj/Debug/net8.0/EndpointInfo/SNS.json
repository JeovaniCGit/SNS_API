        },
        "additionalProperties": false
      },
      "HistoricoLaboralDTO": {
        "required": [
          "dataFim",
          "dataInicio",
          "instituiçãoid"
        ],
        "type": "object",
        "properties": {
          "instituiçãoid": {
            "type": "integer",
            "format": "int32"
          },
          "dataInicio": {
            "type": "string",
            "format": "date-time"
          },
          "dataFim": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },

          }
        },
        "additionalProperties": false
      },
      "UtilizadorUpdateDTO": {
        "type": "object",
        "properties": {
          "nome": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "nTelefone": {
            "pattern": "^[2-9][0-9]{8}$",
            "type": "integer",
            "format": "int32"
          },
          "sexo": {
            "pattern": "^(Masculino|Feminino)$",
            "type": "string",
            "nullable": true
          },
          "morada": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }

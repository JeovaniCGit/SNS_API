  "components": {
    "schemas": {
      "AddAndGetMedicoDataDTO": {
        "required": [
          "especialidadeId",
          "nMedico"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "medicoName": {
            "type": "string",
            "nullable": true
          },
          "nMedico": {
            "pattern": "^\\d{9}$",
            "type": "integer",
            "format": "int32"
          },
          "especialidadeId": {
            "type": "integer",
            "format": "int32"
          },

        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "descri": {
            "type": "string",
            "nullable": true
          },
          "tipoDeSetorid": {
            "type": "integer",
            "format": "int32"
          },
          "tipoDeSetor": {
            "$ref": "#/components/schemas/TipoDeSetor"
          }
        },
        "additionalProperties": false
      },
      "PacienteDTO": {
        "type": "object",
        "properties": {
          "profissao": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "entidadePatronal": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "numeroSns": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "TipoDeSetor": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "descri": {
            "type": "string",
            "nullable": true
          },
          "instituiçãos": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Instituição"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UtilizadorRegistrationDTO": {
        "required": [
          "dataNascimento",
          "morada",
          "nome",
          "nTelefone",
          "numeroCc",
          "sexo",
          "tipoDeUtilizadorid"
        ],
        "type": "object",
        "properties": {
          "nome": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "nTelefone": {
            "pattern": "^[2-9][0-9]{8}$",
            "type": "integer",
            "format": "int32"
          },
          "dataNascimento": {
            "type": "string",
            "format": "date-time"
          },
          "numeroCc": {
            "maximum": 999999999,
            "minimum": 100000000,
            "type": "integer",
            "format": "int32"
          },
          "sexo": {
            "minLength": 1,
            "pattern": "^(Masculino|Feminino)$",
            "type": "string"
          },
          "morada": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string"
          },
          "tipoDeUtilizadorid": {
            "type": "integer",
            "format": "int32"
          },
          "medicoToAttributeId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "pacienteData": {

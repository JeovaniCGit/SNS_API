          "medicoId": {
            "type": "integer",
            "format": "int32"
          },
          "pacienteId": {
            "type": "integer",
            "format": "int32"
          },
          "tipoDeSetorId": {
            "type": "integer",
            "format": "int32"
          },
          "diagnostico": {
            "maxLength": 2000,
            "minLength": 0,
            "type": "string"
          },
          "periodoDeIncapacidade": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "recomendacoes": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "BaixaMedicaUpdateDTO": {

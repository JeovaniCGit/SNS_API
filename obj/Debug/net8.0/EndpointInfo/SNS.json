            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "periodoDeIncapacidade": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "recomendacoes": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
